{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use":"@vercel/node"
            
        }
    ],


    "routes": [
        { 
         "src": "/.*", 
         "dest": "/" }
    ],
    
        "crons": [{
          "path": "/api/cron",
          "schedule": "0 10 * * *"
        }]
     
}